{
    "manifest_version": 2,
    "name": "LinkScrape",
    "version": "1.0",
    "content_scripts": [
      {
        "matches": ["*://www.linkedin.com/feed/*"],
        "js": ["jquery.js","adminMainHandle.js"]
      },
        {
          "matches": ["*://www.linkedin.com/company/*/?viewAsMember=true"],
          "js": ["jquery.js","scrape.js"]
        }
    ],
    "permissions": [
      "webRequest",
      "<all_urls>"
    ]
    
}













